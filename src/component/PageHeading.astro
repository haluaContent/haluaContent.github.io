---

interface Props {

text: String
}

const { text } = Astro.props as Props

---


<div  class="main-heading">

    <h1 class="page-heading montserrat-basic">{text}</h1>

</div>




<style>

* {
    box-sizing: border-box;
}

.main-heading {
text-align: center;
margin-top: 24px;
}

.page-heading {
    font-weight: 400;
    font-size: 1.5vw;
}
</style>

